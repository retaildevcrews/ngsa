    "url": "https://github.com/retaildevcrews/ngsa/issues"
  },
  "homepage": "https://github.com/retaildevcrews/ngsa#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.15",
    "command-line-args": "^5",
    "command-line-usage": "^6",
    "http-status-codes": "^2.1.3",
    "inversify": "^5.0.5",
    "inversify-restify-utils": "^3.4.1",
    "node-cache": "^5.1.2",
    "reflect-metadata": "^0.1.13",
