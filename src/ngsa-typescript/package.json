    "@types/restify": "^8.4.2",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "bufferutil": "^4.0.3",
    "chai": "^4.2.0",
    "eslint": "^7.17.0",
    "gulp": "^4.0.2",
    "mocha": "^8.1.3",
    "mockery": "^2.1.0",
    "node-mocks-http": "^1.9.0",
    "nyc": "^15.1.0",
